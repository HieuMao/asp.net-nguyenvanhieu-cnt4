@model IEnumerable<NvhLesson06.Models.NvhSong>

@{
    ViewBag.Title = "NvhIndex";
}

<h2>NvhIndex</h2>

<p>
    @Html.ActionLink("Create New", "NvhCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NvhTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NvhAuthor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NvhArtist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NvhYearRelease)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NvhTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NvhAuthor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NvhArtist)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NvhYearRelease)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
